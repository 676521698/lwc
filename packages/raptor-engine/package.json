{
  "name": "raptor-engine",
  "version": "0.13.5",
  "description": "Raptor Engine",
  "main": "lib/engine.common.js",
  "module": "lib/engine.es.js",
  "jsnext:main": "lib/engine.es.js",
  "scripts": {
    "clean": "rimraf dist/ lib/",
    "test": "jest --coverage",
    "types": "tsc src/**/*.ts --noEmit",
    "lint": "eslint src/**/*.ts",
    "build": "rollup -c scripts/rollup.config.js",
    "prepublish": "yarn run build"
  },
  "publishConfig": {
    "registry": "https://nexus.soma.salesforce.com/nexus/content/repositories/npmjs-internal/"
  },
  "devDependencies": {
    "cross-env": "^3.1.4",
    "eslint": "^3.19.0",
    "eslint-plugin-typescript": "^0.1.0",
    "expect": "^1.20.2",
    "jest": "^20.0.4",
    "power-assert": "^1.4.2",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.1",
    "rollup-plugin-inject": "^1.4.1",
    "rollup-plugin-strip-caridy-patched": "^1.1.1",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^1.0.1",
    "snabbdom": "^0.5.4",
    "typescript": "^2.3.4",
    "typescript-eslint-parser": "^3.0.0",
    "uglify-es": "^3.0.24"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "/__tests__/",
    "transform": {
      "^.+\\.(ts|js)$": "<rootDir>/scripts/jest-preprocessor.js"
    },
    "setupTestFrameworkScriptFile": "./scripts/jest-env-setup.js",
    "collectCoverageFrom": [
      "src/framework/*.ts"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 65,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
